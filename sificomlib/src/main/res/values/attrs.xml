<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MasterTextField">
        <attr name="editable" format="boolean" />
        <attr name="edittext_type">
            <enum name="none" value="-1" />
            <enum name="password" value="1" />
            <enum name="text" value="2" />
            <enum name="dropdown" value="3" />
        </attr>
        <attr name="dropdown_list_array" format="reference" />
    </declare-styleable>
</resources>